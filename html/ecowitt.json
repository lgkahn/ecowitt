{
  "name": "Ecowitt HTML template repository",
  "templates": {
    "2": "<i class=\"ewi-air\" style=\"color:#${aqiColor}\"></i> ${aqiDanger}<br>PM2.5: ${pm25} &micro;g/m&sup3;<br>AQI: ${aqi}",
    "2B": "<i class=\"ewi-air\" style=\"color:#${aqiColor}\"></i> ${aqiDanger}<br>PM2.5: ${pm25} &micro;g/m&sup3;<br>AQI: ${aqi}<br><i class=\"ewi-batt${batteryIcon}\"></i> ${battery} %",
    "5": "<i class=\"ewi-water\" style=\"color:#${leakColor}\"></i> ${leakMsg}",
    "5B": "<i class=\"ewi-water\" style=\"color:#${leakColor}\"></i> ${leakMsg}<br><i class=\"ewi-batt${batteryIcon}\"></i> ${battery} %"
  },
  "templatesImperial": {
    "0": "<i class=\"ewi-temperature\"></i> ${temperature} &deg;F<br><i class=\"ewi-humidity\"></i> ${humidity} %",
    "0B": "<i class=\"ewi-temperature\"></i> ${temperature} &deg;F<br><i class=\"ewi-humidity\"></i> ${humidity} %<br><i class=\"ewi-batt${batteryIcon}\"></i> ${battery} %",
    "1": "<i class=\"ewi-temperature\"></i> ${temperature} &deg;F<br><i class=\"ewi-humidity\"></i> ${humidity} %<br><i class=\"ewi-pressure\"></i> ${pressure} inHg",
    "1B": "<i class=\"ewi-temperature\"></i> ${temperature} &deg;F<br><i class=\"ewi-humidity\"></i> ${humidity} %<br><i class=\"ewi-pressure\"></i> ${pressure} inHg<br><i class=\"ewi-batt${batteryIcon}\"></i> ${battery} %",
    "3": "<i class=\"ewi-windspeed\"></i> ${windSpeed} mph<br><i class=\"ewi-wind${windCompass}\"></i> ${windDirection} &deg; ${windCompass}<br><i class=\"ewi-rain\"></i> ${rainRate} in/h<br><i class=\"ewi-light\"></i> ${illuminance} lux<br>UV ${ultravioletDanger}",
    "4": "<i class=\"ewi-lightning\"></i> ${lightningDistance} mi<br>${lightningTime}<br>Count: ${lightningCount}",
    "4B": "<i class=\"ewi-lightning\"></i> ${lightningDistance} mi<br>${lightningTime}<br>Count: ${lightningCount}<br><i class=\"ewi-batt${batteryIcon}\"></i> ${battery} %",
    "6": "<i class=\"ewi-temperature\"></i>${temperature} &deg;F<br><i class=\"ewi-humidity\"></i>${humidity} %<br><i class=\"ewi-dew\"></i>${dewPoint} &deg;F<br><i class=\"ewi-heat\"></i>${simmerIndex} SSI<br><p style=\"color:#${simmerColor}\">${simmerDanger}</p>"
  },
  "templatesMetric": {
    "0": "<i class=\"ewi-temperature\"></i> ${temperature} &deg;C<br><i class=\"ewi-humidity\"></i> ${humidity} %",
    "0B": "<i class=\"ewi-temperature\"></i> ${temperature} &deg;C<br><i class=\"ewi-humidity\"></i> ${humidity} %<br><i class=\"ewi-batt${batteryIcon}\"></i> ${battery} %",
    "1": "<i class=\"ewi-temperature\"></i> ${temperature} &deg;C<br><i class=\"ewi-humidity\"></i> ${humidity} %<br><i class=\"ewi-pressure\"></i> ${pressure} hPa",
    "1B": "<i class=\"ewi-temperature\"></i> ${temperature} &deg;C<br><i class=\"ewi-humidity\"></i> ${humidity} %<br><i class=\"ewi-pressure\"></i> ${pressure} hPa<br><i class=\"ewi-batt${batteryIcon}\"></i> ${battery} %",
    "3": "<i class=\"ewi-windspeed\"></i> ${windSpeed} km/h<br><i class=\"ewi-wind${windCompass}\"></i> ${windDirection} &deg; ${windCompass}<br><i class=\"ewi-rain\"></i> ${rainRate} mm/h<br><i class=\"ewi-light\"></i> ${illuminance} lux<br>UV ${ultravioletDanger}",
    "4": "<i class=\"ewi-lightning\"></i> ${lightningDistance} km<br>${lightningTime}<br>Count: ${lightningCount}",
    "4B": "<i class=\"ewi-lightning\"></i> ${lightningDistance} km<br>${lightningTime}<br>Count: ${lightningCount}<br><i class=\"ewi-batt${batteryIcon}\"></i> ${battery} %",
    "6": "<i class=\"ewi-temperature\"></i>${temperature} &deg;C<br><i class=\"ewi-humidity\"></i>${humidity} %<br><i class=\"ewi-dew\"></i>${dewPoint} &deg;C<br><i class=\"ewi-heat\"></i>${simmerIndex} SSI<br><p style=\"color:#${simmerColor}\">${simmerDanger}</p>"
  }
}
